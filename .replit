modules = ["nodejs-20", "python-3.12", "web"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start application"

[[workflows.workflow]]
name = "Start application"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd \"05_EXPORT/BRUCE_BRUCE__CODEX/harriswildlands.com github repo/harriswildlands.com-main\" && npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[deployment]
deploymentTarget = "autoscale"
run = ["bash", "-c", "cd '05_EXPORT/BRUCE_BRUCE__CODEX/harriswildlands.com github repo/harriswildlands.com-main' && npm run start"]
build = ["bash", "-c", "cd '05_EXPORT/BRUCE_BRUCE__CODEX/harriswildlands.com github repo/harriswildlands.com-main' && npm run build"]
